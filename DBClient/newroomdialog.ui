    border-radius: 5px;
    color: #ffffff;            /* 白色文字 */
    font-family: &quot;Microsoft YaHei&quot;;
    font-size: 14px;
    padding: 5px 10px;
    min-height: 30px;
}

/* 下拉框聚焦时的样式 */
QComboBox#canvasSize_cbx:focus {
    border: 1px solid #bba47c; /* 聚焦时变为金色边框 */
}

/* 下拉框的下拉箭头区域样式 */
QComboBox::drop-down#canvasSize_cbx {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left-width: 0px; /* 不显示左边框 */
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
}

/* 下拉列表的样式 */
QComboBox QAbstractItemView#canvasSize_cbx {
    background-color: #141b2d; /* 下拉列表背景 */
    border: 1px solid #bba47c; /* 下拉列表边框 */
    color: #ffffff;            /* 下拉列表文字颜色 */
